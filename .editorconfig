# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
charset = utf-8
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true

[*.{c,h,cpp,hpp,esp}]
indent_size = 4

[*.js]
indent_size = 2

[*.html]
indent_size = 4

[*.bin]
generated_code = true

[*.min.*]
generated_code = true
